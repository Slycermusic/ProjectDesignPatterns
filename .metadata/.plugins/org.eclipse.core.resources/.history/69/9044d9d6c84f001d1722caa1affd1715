public class Main {
	public static void main(String[] args) {
	      //get shape factory
	      main.factory.AbstractFactory shapeFactory = FactoryProducer.getFactory(false);
	      //get an object of main.factory.Shape Rectangle
	      main.factory.Shape shape1 = shapeFactory.getShape("RECTANGLE");
	      //call draw method of main.factory.Shape Rectangle
	      shape1.draw();
	      //get an object of main.factory.Shape Square
	      main.factory.Shape shape2 = shapeFactory.getShape("SQUARE");
	      //call draw method of main.factory.Shape Square
	      shape2.draw();
	      //get shape factory
	      main.factory.AbstractFactory shapeFactory1 = FactoryProducer.getFactory(true);
	      //get an object of main.factory.Shape Rectangle
	      main.factory.Shape shape3 = shapeFactory1.getShape("RECTANGLE");
	      //call draw method of main.factory.Shape Rectangle
	      shape3.draw();
	      //get an object of main.factory.Shape Square
	      main.factory.Shape shape4 = shapeFactory1.getShape("SQUARE");
	      //call draw method of main.factory.Shape Square
	      shape4.draw();
	}
}


public class ShapeFactory extends main.factory.AbstractFactory {

	@Override
	public main.factory.Shape getShape(String shapeType) {
	      if(shapeType.equalsIgnoreCase("RECTANGLE")){
	          return new Rectangle();         
	       }else if(shapeType.equalsIgnoreCase("SQUARE")){
	          return new Square();
	       }	 
	       return null;
	    }
	}

}
